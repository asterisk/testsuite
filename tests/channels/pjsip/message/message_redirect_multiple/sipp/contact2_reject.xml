<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">

<scenario name="MESSAGE contact2 rejects">
  <recv request="MESSAGE" crlf="true">
    <action>
      <!-- Verify this is the redirected MESSAGE to contact2 -->
      <ereg search_in="msg" regexp="MESSAGE sip:contact2@"
            check_it="true" assign_to="junk1"/>
      <ereg search_in="body" regexp="Iteration [0-9]+:"
            check_it="true" assign_to="junk2"/>
    </action>
  </recv>
  <Reference variables="junk1"/>
  <Reference variables="junk2"/>

  <send>
    <![CDATA[

      SIP/2.0 404 Not Found
      [last_Via:]
      [last_From:]
      [last_To:];tag=[call_number]
      [last_Call-ID:]
      [last_CSeq:]
      Content-Length: 0

    ]]>
  </send>

  <pause milliseconds="500" />

</scenario>
